{
  "name": "extension-boilerplate",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && node scripts/package.js",
    "document-session": "node scripts/document-session.js",
    "document-session:interactive": "node scripts/document-session.js --interactive",
    "document-session:template": "node scripts/document-session.js --template-only",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write ."
  },
  "devDependencies": {
    "@churchtools/churchtools-client": "^1.4.0",
    "@types/node": "^24.3.0",
    "@vue/compiler-sfc": "^3.5.21",
    "prettier": "^3.3.3",
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "vue": "^3.5.21",
    "vue-i18n": "^9.14.5"
  }
}